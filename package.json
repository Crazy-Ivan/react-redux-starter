{
  "name": "react-redux-starter.git",
  "version": "1.0.0",
  "description": "Get started with React, Redux, and React-Router!",
  "main": "index.js",
  "engines": {
    "node": ">=4.2.0",
    "npm": "^3.0.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Crazy-Ivan/react-redux-starter.git"
  },
  "scripts": {
    "start:dev": "better-npm-run start:dev",
    "start:prod": "better-npm-run start:prod",
    "lint": "eslint app server build",
    "lint:fix": "npm run lint -- --fix",
    "clean": "rimraf dist",
    "build": "better-npm-run build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "betterScripts": {
    "start:dev": {
      "command": "npm run clean && nodemon --watch server --exec babel-node server/app",
      "env": {
        "NODE_ENV": "development",
        "DEBUG": "app:*"
      }
    },
    "start:prod": {
      "command": "babel-node server/app",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    },
    "build": {
      "command": "npm run clean && babel-node build/bin/compile",
      "env": {
        "NODE_ENV": "production",
        "DEBUG": "app:*"
      }
    }
  },
  "pre-commit": ["lint"],
  "author": "Paweł Bród",
  "license": "ISC",
  "devDependencies": {
    "babel-cli": "^6.10.1",
    "babel-core": "^6.11.4",
    "babel-eslint": "^6.1.2",
    "babel-loader": "^6.2.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-runtime": "^6.11.6",
    "better-npm-run": "0.0.9",
    "css-loader": "^0.23.1",
    "debug": "^2.2.0",
    "eslint": "^3.2.2",
    "eslint-plugin-promise": "^2.0.0",
    "eslint-plugin-react": "^6.0.0",
    "fs-extra": "^0.30.0",
    "html-webpack-plugin": "^2.22.0",
    "json-loader": "^0.5.4",
    "nodemon": "^1.10.0",
    "pre-commit": "^1.1.3",
    "rimraf": "^2.5.4",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-middleware": "^1.6.1",
    "webpack-hot-middleware": "^2.12.2"
  },
  "dependencies": {
    "architect": "^0.1.11",
    "express": "^4.14.0",
    "morgan": "^1.7.0",
    "path": "^0.12.7",
    "react": "^15.2.1",
    "react-dom": "^15.3.0",
    "react-redux": "^4.4.5",
    "react-router": "^2.5.2",
    "react-router-redux": "^4.0.5",
    "redbox-react": "^1.2.10",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
